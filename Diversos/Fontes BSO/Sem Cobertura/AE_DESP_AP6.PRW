Template Function VldExcDp()

Local cCodDes:= LHS->LHS_CODIGO 
Local lRet := .T.
Local cAliasTrb :="TMPLHR"
Local aSaveArea := GetArea()  

	BeginSql Alias cAliasTrb 
		SELECT LHR_CODDES 
		FROM %table:LHR%
	    WHERE LHR_FILIAL = %xFilial:LHR% 
		AND LHR_CODDES = %exp:cCodDes% 
		AND %NotDel%
	EndSQL
	
	If (cAliasTrb)->(! Eof())
		MsgInfo(STR0023)
		lRet := .F.		
	EndIf
	
  (cAliasTrb)->(DbCloseArea())
  
RestArea(aSaveArea)   

Return lRet