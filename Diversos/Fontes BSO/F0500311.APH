            <label><%="Filial Atual"%></label>
            <input type="text" name="cTMPFILIAL" class="Texto" size="	6" disabled="true" value="<%=cTMPFILIAL%>">
            <label><%="Nome da Filial Atual"%></label>
            <input type="text" name="cTMPD_FILI" class="Texto" size="	50" disabled="true" value="<%=cTMPD_FILI%>">
            <label><%="Centro de Custo Atual"%></label>
