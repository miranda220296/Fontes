            <label><%="Posto Proposto"%></label>
            <input type="text" name="cRAPOSTO" class="Texto" size="	9" disabled="true" value="<%=cRAPOSTO%>">
            <% EndIf %>
            <% EndIf %>				
            <br>
            <% If !(EMPTY(cTMPCLVL)) %>				
